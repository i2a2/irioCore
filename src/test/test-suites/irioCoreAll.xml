<?xml version="1.0" encoding="UTF-8"?>
<testplan
    xmlns="http://www.testLocation.com/test"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.testLocation.com/test ../testSchema.xsd">

    <!-- The IMAQ functional tests are grouped together to avoid
         the need of resetting the image send program -->

    <!--==================== IRIO CORE UNITARY ======================= -->
    <test>
        <name>irioCoreUnit</name>
        <TestType>irioCore Unitary</TestType>
        <TestFilter>*</TestFilter>
        <RIODevice>7966</RIODevice>
        <RIOSerial>0xFFFFFFFF</RIOSerial>
        <RIOModule>noModule</RIOModule>
        <verbose>true</verbose>
    </test>
    <!--==================== IRIO CORE CPP UNITARY =================== -->
    <test>
        <name>irioCoreCppUnit</name>
        <TestType>irioCoreCpp Unitary</TestType>
        <TestFilter>*</TestFilter>
        <RIODevice>7966</RIODevice>
        <RIOSerial>0xFFFFFFFF</RIOSerial>
        <RIOModule>noModule</RIOModule>
        <verbose>true</verbose>
    </test>
    <!--==================== IRIO CORE FUNCTIONAL ==================== -->
    <test>
        <name>Common FlexRIO</name>
        <TestFilter>Common.*</TestFilter>
        <TestType>irioCore Functional</TestType>
        <RIODevice>7966</RIODevice>
        <RIOSerial>0x0177A2AD</RIOSerial>
        <RIOModule>noModule</RIOModule>
        <verbose>true</verbose>
    </test>
